<div class="all">
  <div class="container">
    <h1>ユーザ一覧</h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>ユーザ名</th>
                <th>メールアドレス</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
          <% @users.each do |user|%>
            <%= render user %>
          <% end %>
        </tbody>
    </table>
    <%= paginate @users %>
  </div>

</div>
